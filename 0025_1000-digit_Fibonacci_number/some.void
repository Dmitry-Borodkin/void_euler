{   v_import("imports.void");
}

{   enable_imports(); }


//---------------------------------------------------------------------
num_t = uint(3322);         //- kinda  1000 * ln(10) / ln(2) ...


//---------------------------------------------------------------------
pow_num_t: (a: num_t, n: unsigned) -> num_t
{
    if (n == 0) v_return(1);
    if (n == 1) v_return(a);

    ret: &num_t := pow_num_t(a, n/2);

    ret := ret * ret;

    if (n & 1)  ret *= a;

    v_return(ret);
}


//---------------------------------------------------------------------
ten_pow_999: &num_t := v_undef();

{   ten_pow_999 := pow_num_t(10, 999);
}


//---------------------------------------------------------------------
{
    i0: &num_t := 0;
    i1: &num_t := 1;

    n: &int := 2;

    loop
    {
        i = i0 + i1;

        if (i >= ten_pow_999) v_break();

        i0 := i1;
        i1 := i;

        ++n;
    }

    printf("%d\n", n);
}


