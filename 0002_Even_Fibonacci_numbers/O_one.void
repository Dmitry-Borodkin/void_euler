//---------------------------------------------------------------------
Sr5: &double := {};
phi: &double := {};

h: &double := {};

log_phi: &double := {};

{   Sr5 := sqrt(5);

    phi := (1 + Sr5) / 2;

    h := (1:double) / 2;

    log_phi := log(phi);
}

O_one: (N: num_t) -> num_t
{
    P = log(N*Sr5 + h) / log_phi;

    P3 = (P: unsigned) / 3;

    dphiSr5_3 = powSr5(dphiSr5, 3);
    dpsiSr5_3 = powSr5(dpsiSr5, 3);

    Sdphi_P3 = sumGpSr5(dphiSr5_3, 1<<3, P3);
    Sdpsi_P3 = sumGpSr5(dpsiSr5_3, 1<<3, P3);

    r = Sdphi_P3 - Sdpsi_P3;

    r = (r[1] : uint(128)) >> 3*P3;

    v_return(r);
}


//---------------------------------------------------------------------
N: &num_t := {};
R: &num_t := {};

doit: () -> void
{
    R := O_one(N);
}


//---------------------------------------------------------------------
{
    NC = 1000000;


    t0 = checktime(donothing, NC);

    printf("t0: %g\n", t0);

    fflush(stdout);


    NS: num_t = 4000000;

    for (N := NS; N <= NS*1000*1000*100; N *= 10)
    {
        t = checktime(doit, NC);

        printf("N: %lld, R: %lld, t: %g, (t-t0): %g\n", (N: long_long), (R: long_long), t, t-t0);

        fflush(stdout);
    }
}


