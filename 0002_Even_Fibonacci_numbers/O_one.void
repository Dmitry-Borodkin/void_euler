//---------------------------------------------------------------------
sumGpSr5: (qn: numSr5_t, qd: num_t, n: unsigned) -> numSr5_t
{
    num = powSr5(qn, n+1) - oneSr5*(qd << (n+1));

    den = qn - oneSr5*qd;

    ret = divSr5(num, den);

    v_return(ret);
}

