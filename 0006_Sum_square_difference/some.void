{   v_import("imports.void");
}

{   enable_imports(); }


//---------------------------------------------------------------------
some: (n: int) -> int
{
    s1 = n*(n+1) / 2;

    s2 = n*(n+1)*(2*n+1) / 6;

//  printf("s1: %d, s2: %d\n", s1, s2);

    v_return(s1*s1 - s2);
}


//---------------------------------------------------------------------
{
    printf("%d\n", some(100));
}






