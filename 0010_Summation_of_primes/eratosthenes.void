{   v_import("imports.void");
}

{   enable_imports(); }


//---------------------------------------------------------------------
sqrt: (float(64)) -> float(64);
ceil: (float(64)) -> float(64);


//---------------------------------------------------------------------
//N = 10;
  N = 2000000;

{   printf("N: %d\n", N); }


//---------------------------------------------------------------------
sieve: &*bool := {};

{   sieve := v_malloc(bool, N);

    memset(sieve, 0, N);

    sieve[2] := true;

    for (i: &int := 3; i < N; i += 2)
    {
        sieve[i] := true;
    }

    R = (ceil(sqrt(N)) : int);

    for (i: &int := 3; i < R; i += 2)
    {
        if (sieve[i])
        {
            for (k: &int := i*i, i2 = 2*i; k < N; k += i2)
            {
                sieve[k] := false;
            }
        }
    }
}


//---------------------------------------------------------------------
{
    sum: &long := 2;

    for (p: &int := 3; p < N; p += 2)
    {
        if (sieve[p])   sum += p;
    }

    printf("sum: %ld\n", sum);
}


//---------------------------------------------------------------------
{
    v_free(sieve);
}

