{   v_import("imports.void");
}

{   enable_imports(); }


//---------------------------------------------------------------------
coins: &int[8] := {1, 2, 5, 10, 20, 50, 100, 200};


//---------------------------------------------------------------------
compute: (i: int, p: int) ~> int
{
    if (p == 0) v_return(1);
    if (i == 0) v_return(1);
    if (i == 1) v_return(p/2 + 1);

    s: &int := 0;

    for (pp: &int := p; pp >= 0; pp -= coins[i])
    {
        s += compute(i-1, pp);
    }

    v_return(s);
}


//---------------------------------------------------------------------
{
    s = compute(7, 200);

    printf("%d\n", s);
}


